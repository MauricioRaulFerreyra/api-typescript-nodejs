{
  "name": "nodejs-typescript",
  "version": "1.0.0",
  "description": "API REST Nodejs with Typescript",
  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "start": "tsc && node dist/server.js",
    "start:dev": "tsc && concurrently \"tsc -w\" \"nodemon dist/server.js\"",
    "start:prod": "SET NODE_ENV=production && npm start",
    "typeorm": "typeorm-ts-node-esm",
    "m:show": "typeorm migration:show -- -d dist/config/data.source.js",
    "m:gen": "typeorm migration:generate src/migrations/ChangesDB -d dist/config/data.source.js",
    "m:run": "ts-node ./node_modules/typeorm/cli.js migration:run -d ./dist/config/data.source.js",
    "m:revert": "ts-node ./node_modules/typeorm/cli.js migration:revert -d ./dist/src/config/data.source.js"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/cors": "2.8.17",
    "@types/dotenv": "^8.2.0",
    "@types/express": "4.17.21",
    "@types/morgan": "1.9.9",
    "@types/passport-jwt": "4.0.1",
    "@types/passport-local": "1.0.38",
    "concurrently": "8.2.2",
    "nodemon": "3.1.3",
    "ts-node-dev": "2.0.0",
    "typescript": "5.4.5"
  },
  "dependencies": {
    "bcrypt": "5.1.1",
    "class-transformer": "0.5.1",
    "class-validator": "0.14.1",
    "cors": "2.8.5",
    "dotenv": "16.4.5",
    "express": "4.19.2",
    "morgan": "1.10.0",
    "mysql2": "3.10.0",
    "passport": "0.7.0",
    "passport-jwt": "4.0.1",
    "passport-local": "1.0.0",
    "reflect-metadata": "0.2.2",
    "ts-node": "10.9.2",
    "tsconfig-paths": "4.2.0",
    "typeorm": "0.3.20",
    "typeorm-naming-strategies": "4.1.0"
  }
}
